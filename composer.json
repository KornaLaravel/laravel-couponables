{
    "name": "michael-rubel/laravel-couponables",
    "description": "This package provides polymorphic coupon functionality for your Laravel application.",
    "keywords": [
        "michael-rubel",
        "laravel",
        "laravel-couponables"
    ],
    "homepage": "https://github.com/michael-rubel/laravel-couponables",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Rubel",
            "email": "contact@observer.name",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "laravel/framework": "^8.71|^9.0",
        "spatie/laravel-package-tools": "^1.10",
        "michael-rubel/laravel-enhanced-container": "^6.0|^7.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10|^6.0",
        "nunomaduro/larastan": "^1.0",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.6|^7.0",
        "brianium/paratest": "^6.3",
        "mockery/mockery": "^1.4.4",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "MichaelRubel\\Couponables\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichaelRubel\\Couponables\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichaelRubel\\Couponables\\CouponableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
